deftemplate Employee_evaluation
	(slot work_experience)
	(slot stressresistance)
	(slot Programming_Skills)
	(slot Knowledge)
        )
(deffacts Allfacts
	(Employee_evaluation (work_experience 1_year_and_more))
	(Employee_evaluation (stressresistance yes))
	(Employee_evaluation (Programming_Skills Java,C#))
	(Employee_evaluation (Knowledge SQL)))
(defrule 1_Engineer_3_level
        (Employee_evaluation (work_experience 1_year_and_more))
	(Employee_evaluation (stressresistance yes))
	(Employee_evaluation (Programming_Skills Java))
	(Employee_evaluation (Knowledge SQL))
=> (printout t Engineer_3_level crlf)
)
(defrule 2_Engineer_1_level
        (Employee_evaluation (work_experience 1_year_and_more))
	(Employee_evaluation (stressresistance yes))
	(Employee_evaluation (Programming_Skills No))
	(Employee_evaluation (Knowledge No))
=> (printout t Engineer_1_level crlf)
)
(defrule 3_Engineer_2_level
        (Employee_evaluation (work_experience 1_year_and_more))
	(Employee_evaluation (stressresistance yes))
	(Employee_evaluation (Programming_Skills Prolog))
	(Employee_evaluation (Knowledge No))
=> (printout t Engineer_2_level crlf)
)
(defrule 4_Engineer_2_level
        (Employee_evaluation (work_experience 1_year_and_more))
	(Employee_evaluation (stressresistance yes))
	(Employee_evaluation (Programming_Skills C#))
	(Employee_evaluation (Knowledge No))
=> (printout t Engineer_2_level crlf)
)
(defrule 5_Engineer_3_level
        (Employee_evaluation (work_experience 1_year_and_more))
	(Employee_evaluation (stressresistance yes))
	(Employee_evaluation (Programming_Skills Java,C#))
	(Employee_evaluation (Knowledge No))
=> (printout t Engineer_3_level crlf)
)
(defrule 6_Engineer_Leader
        (Employee_evaluation (work_experience 1_year_and_more))
	(Employee_evaluation (stressresistance yes))
	(Employee_evaluation (Programming_Skills Java,C#,Prolog))
	(Employee_evaluation (Knowledge SQL))
=> (printout t Engineer_Leader crlf)
)
(defrule 8Refuse
	(or
	(Employee_evaluation (work_experience less_1_year))
	(Employee_evaluation (stressresistance no))
	)	
	=> (printout t Refuse crlf)
)